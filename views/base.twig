{# Imports #}
{% import 'components/image.twig' as Logo %}

{# Blocks & Includes #}
{% block html_head_container %}
	{% include 'partials/html-header.twig' %}
	{% block head %}{% endblock %}
</head>{% endblock %}<body class="{{body_class}}" data-template="base.twig">
<a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content')}}</a>
<a href="javascript:" id="return-to-top">
	<i class="glyphicon glyphicon-chevron-up" aria-hidden="true"></i>
</a>
{% block header %}
	<header class="header padding-lr">
		<div class="wrapper container">
			<a class="site-logo" href="{{site.url}}" rel="home" aria-label="site logo">
				{{source (directory ~ '/static/images/odisi-logo.svg') }}
			</a>

			<nav id="site-navigation" class="nav-main" role="navigation">
				{% include "menu.twig" with{'items':menu.get_items} %}
			</nav>
			<div class="button_container" id="toggle" data-aos="fade-zoom-in" data-aos-easing="ease-in-back">
				<span class="top"></span>
				<span class="middle"></span>
				<span class="bottom"></span>
			</div>
		</div>
	</header>
{% endblock %}
<div id="smooth-wrapper">
	<div
		id="smooth-content">

		{# Include Hero #}
		{% include 'partials/hero.twig' %}

		<section id="content" role="main" class="content-wrapper">
			<div class="wrapper{{sidebar_class}}"> {% block content %}{% endblock %}
				</div>
			</section>


			<div class="overlay padding-lr" id="overlay">
				<div class="wrapper container">
					<a
						class="site-logo" href="{{site.url}}" rel="home" aria-label="{{site.name}}">{# {{source (directory ~ '/images/logo/procast-logo.svg') }} #}
					</a>
					<div class="overlay-nav-main">
						<nav id="site-navigation3" class="overlay-menu" role="navigation">
							{% include "menu.twig" with{'items':menu.get_items} %}
						</nav>
					</div>
				</div>
			</div>

			{% block footer %}
				<footer id="footer">
					{% include 'partials/footer.twig' %}
				</footer>
				{{ function('wp_footer')}}
			{% endblock %}
			<script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
		</div>
	</div>

</body></html>
